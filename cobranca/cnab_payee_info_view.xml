<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    
        <record id="cnab_payee_info_form" model="ir.ui.view">
            <field name="name">CNAB Payee Info</field>
            <field name="model">cnab.payee_info</field>
            <field name="arch" type="xml">
                <form string="Informação do Recebedor">
                    <group string="Códigos" colspan="2">
                        <field name="codigo_empresa"/>
                        <field name="codigo_carteira"/>
                        <field name="codigo_agencia"/>
                    </group>
                    <group string="Conta" colspan="2">
                        <field name="numero_conta"/>
                        <field name="digito_conta"/>
                    </group>
                    <group string="Débito Automático" colspan="2">
                        <field name="aviso_debito_automatico"/>
                        <field name="registro_debito_automatico"/>
                    </group>
                    <group string="Outros" colspan="2">
                        <field name="rateio_credito"/>
                    </group>
                </form>
            </field>
        </record>
        
    </data>
</openerp>
